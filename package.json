{
  "name": "spectral-lab",
  "version": "0.0.1",
  "author": "spectral-lab",
  "description": "An electron-vue project",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "scripts": {
    "postinstall": "[ -d static/server/venv ] || npm run install:server",
    "install:server": "cd static/server && python3 -m venv venv && ./venv/bin/python3 -m pip install -r requirements.txt",
    "build": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter {src,test}",
    "lint:watch": "esw -w --color --changed --clear --ext .js,.vue -f ./node_modules/eslint-friendly-formatter {src,test}",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix {src,test}",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "test": "jest --watch",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "type-check": "flow-watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && flow",
      "pre-push": "git branch | grep \"*\" | grep -v master"
    }
  },
  "lint-staged": {
    "{src,test}/**/*.{js,vue}": [
      "eslint -f ./node_modules/eslint-friendly-formatter --fix",
      "git add"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?)$"
  },
  "build": {
    "productName": "Spectral Lab",
    "appId": "com.example.yourapp",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "identity": null,
      "publish": null,
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.9.0",
    "@vuex-orm/core": "^0.34.1",
    "change-case": "^3.1.0",
    "color": "^3.1.2",
    "decibels": "^2.0.0",
    "dsp.js-browser": "^1.0.1",
    "element-resize-detector": "^1.1.15",
    "express": "^4.16.4",
    "fs-extra": "^8.1.0",
    "hotkeys-js": "^3.6.11",
    "jzz": "^0.8.5",
    "jzz-midi-smf": "^1.2.7",
    "lodash": "^4.17.13",
    "material-design-icons-iconfont": "^5.0.1",
    "pngjs": "^3.4.0",
    "svg.js": "^2.7.1",
    "teoria": "^2.5.0",
    "uid": "^0.0.2",
    "vue": "^2.6.10",
    "vue-context-menu": "^2.0.6",
    "vue-electron": "^1.0.6",
    "vue-eslint-parser": "^6.0.4",
    "vuetify": "1.5.16",
    "vuetify-upload-button": "^2.0.0",
    "vuex": "^3.0.1",
    "vuex-electron": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-function-bind": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-flow": "^7.7.4",
    "@babel/register": "^7.7.7",
    "@babel/runtime": "^7.7.7",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-links": "^5.1.11",
    "@storybook/addons": "^5.1.11",
    "@storybook/vue": "^5.1.11",
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "^24.0.16",
    "@vue/test-utils": "^1.0.0-beta.29",
    "ajv": "^6.5.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-preset-vue": "^2.0.2",
    "cfonts": "^2.1.2",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^5.0.5",
    "cross-env": "^5.1.6",
    "css-loader": "^2.1.1",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^13.6.6",
    "electron-builder": "^21.2.0",
    "electron-debug": "^1.5.0",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^6.1.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-flowtype": "^4.2.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.3",
    "file-loader": "^1.1.11",
    "flow-bin": "^0.105.2",
    "flow-watch": "^1.1.4",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.4.0",
    "jest": "^24.9.0",
    "jest-serializer-vue": "^2.0.2",
    "lint-staged": ">=8",
    "mini-css-extract-plugin": "0.4.0",
    "multispinner": "https://github.com/spectral-lab/node-multispinner",
    "node-loader": "^0.6.0",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "vue-html-loader": "^1.2.4",
    "vue-jest": "^3.0.4",
    "vue-loader": "^15.2.4",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.3"
  }
}
